{
  "name": "magtifunJS",
  "version": "1.0",
  "manifest_version": 2,
  "description": "send sms easely",
  "background": {
    "scripts": ["eventPage.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "logo.ico",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}